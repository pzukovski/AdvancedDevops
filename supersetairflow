apiVersion: v1
kind: Service
metadata:
  name: superset
spec:
selector:
name: superset
type: NodePort
ports:
- name: superset
port: 9999
targetPort: 9999
---
apiVersion: v1
kind: Pod
metadata:
  name: superset
labels:
  name: supersetairflow
spec:
containers:
- image: amancevice/superset
name: superset
---
